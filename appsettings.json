{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ConnectionStrings": {
      "type": "object",
      "properties": {
        "CodeWeTrustDb": {
          "type": "string"
        }
      }
    },
    "AllowedHosts": {
      "type": "string"
    },
    "JiraConnectorSettings": {
      "type": "object",
      "properties": {
        "Username": {
          "type": "string"
        },
        "ApiToken": {
          "type": "string"
        },
        "BaseUrl": {
          "type": "string"
        },
        "ProjectKey": {
          "type": "string"
        }
      }
    },
    "CodeWeTrustSettings": {
      "type": "object",
      "properties": {
        "AnalysisDisabled": {
          "type": "boolean"
        },
        "ReadOnly": {
          "type": "boolean"
        },
        "SkipBrowserOpen": {
          "type": "boolean"
        },
        "SkipSystemCheck": {
          "type": "boolean"
        },
        "DisableWebView": {
          "type": "boolean"
        },
        "SkipFrontEndFiles": {
          "type": "boolean"
        },
        "AllowMultipleInstances": {
          "type": "boolean"
        },
        "CacheExpirationTimeout": {
          "type": "string"
        }
      }
    },
    "AuthorizationSettings": {
      "type": "object",
      "properties": {
        "Administrators": {
          "type": "string"
        },
        "Users": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "Email": {
                  "type": "string"
                },
                "IsAdmin": {
                  "type": "string"
                },
                "Permissions": {
                  "type": "string"
                }
              }
            }
          ]
        }
      }
    },
    "RestApiSettings": {
      "type": "object",
      "properties": {
        "Enabled": {
          "type": "boolean"
        }
      }
    },
    "Logging": {
      "type": "object",
      "properties": {
        "LogLevel": {
          "type": "object",
          "properties": {
            "Default": {
              "type": "string"
            }
          }
        }
      }
    },
    "Kestrel": {
      "type": "object",
      "properties": {
        "EndPoints": {
          "type": "object",
          "properties": {
            "Http": {
              "type": "object",
              "properties": {
                "Url": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
